{
  "name": "palmind-chat-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:dev": "ES_LINT_NO_DEV_ERRORS=true BROWSER=none vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest watch ",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage ",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts, tsx, css, scss}\"",
    "check-bundle": "MODE=production npx vite-bundle-visualizer"
  },
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "dependencies": {
    "@types/dot-object": "^2.1.6",
    "antd": "^5.24.2",
    "axios": "^1.7.9",
    "axios-mock-adapter": "^1.21.5",
    "dayjs": "^1.11.13",
    "dot-object": "^2.1.4",
    "express": "^4.21.2",
    "flowbite": "^1.7.0",
    "formik": "^2.4.2",
    "html2canvas": "^1.4.1",
    "latest-version": "^7.0.0",
    "moment": "^2.29.4",
    "motion": "^12.6.0",
    "number-to-words": "^1.2.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-barcode": "^1.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.1",
    "react-to-print": "^3.0.5",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/number-to-words": "^1.2.3",
    "@types/react": "^18.2.14",
    "@types/react-datepicker": "^4.19.1",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "3.1.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.9",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": "v22.14.0",
    "npm": "10.9.2"
  }
}
